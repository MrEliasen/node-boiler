{
    "extends": [
        "google",
        "plugin:security/recommended"
    ],
    "env": {
        "browser": false,
        "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "quotes": "off",
        "spaced-comment": "off",
        "indent": [1, 4, { "SwitchCase": 1 }],
        "no-invalid-this": "off"
    },
    "plugins": [
        "security"
    ],
    "settings": {
        "import/resolver": {
            "babel-module": {}
        }
    },
}